# Copy this file to .env and fill in your credentials


# ClimateGPT API Configuration (Optional - for enhanced climate analysis)
CLIMATEGPT_URL=<ClimateGPT_API_endpoint>
CLIMATEGPT_USER=<username>
CLIMATEGPT_PASSWORD=<password>
CLIMATEGPT_MODEL=<model_name>

# Database Configuration
DATABASE_PATH=/app/data/processed/forest.db

# Project Configuration
PROJECT_ROOT=/app
DATA_DIR=/app/data
CONFIG_DIR=/app/config

# Optional: Excel File Name (if using different dataset)
# EXCEL_FILE=your_data_file.xlsx

# Logging Configuration
LOG_LEVEL=INFO

# For Global Forest Watch data, see: https://www.globalforestwatch.org/